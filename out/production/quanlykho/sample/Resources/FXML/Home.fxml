<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="800.0" stylesheets="@../CSS/Home.css"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="sample.Controller.HomeController">
    <children>
        <BorderPane prefHeight="600.0" prefWidth="800.0">
            <left>
                <VBox fx:id="splitVbox" prefHeight="800.0" prefWidth="112.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="homeButton" contentDisplay="TOP" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#homeButtonOnClick"
                                prefHeight="80.0" prefWidth="100.0" text="Thống kê">
                            <graphic>
                                <FontAwesomeIcon glyphName="LINE_CHART" size="1.5em"/>
                            </graphic>
                        </Button>
                        <Button contentDisplay="TOP" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#intvetoryOnClick"
                                prefHeight="80.0" prefWidth="100.0" text="Hàng tồn kho">
                            <graphic>
                                <FontAwesomeIcon glyphName="CUBES" size="1.5em"/>
                            </graphic>
                        </Button>
                        <Button contentDisplay="TOP" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sellOnClick"
                                prefHeight="63.0" prefWidth="103.0" text="Xuất hàng">
                            <graphic>
                                <FontAwesomeIcon glyphName="ARROW_UP" size="1.5em"/>
                            </graphic>
                        </Button>
                        <Button contentDisplay="TOP" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#nhapHang"
                                prefHeight="80.0" prefWidth="100.0" text="Nhập hàng">
                            <graphic>
                                <FontAwesomeIcon glyphName="CART_ARROW_DOWN" size="1.5em"/>
                            </graphic>
                        </Button>
                        <Button contentDisplay="TOP" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#listKH"
                                prefHeight="80.0" prefWidth="100.0" text="Khách hàng">
                            <graphic>
                                <FontAwesomeIcon glyphName="MALE" size="1.5em"/>
                            </graphic>
                        </Button>
                        <Button contentDisplay="TOP" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#listNCC"
                                prefHeight="73.0" prefWidth="100.0" text="Nhà cung cấp">
                            <graphic>
                                <FontAwesomeIcon glyphName="TRUCK" size="1.5em"/>
                            </graphic>
                        </Button>
                        <Button fx:id="admin" contentDisplay="TOP" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#adminOnClick"
                                prefHeight="80.0" prefWidth="100.0" text="Quản lí nhân viên">
                            <graphic>
                                <FontAwesomeIcon glyphName="USERS" size="1.5em"/>
                            </graphic>
                        </Button>
                        <AnchorPane prefHeight="113.0" prefWidth="112.0">
                            <children>
                                <Label fx:id="username" layoutX="23.0" layoutY="22.0" prefHeight="17.0" prefWidth="60.0"
                                       text="Tên đăng nhập"/>
                                <Label fx:id="clock" layoutX="23.0" layoutY="67.0" text="Ngày giờ"/>
                            </children>
                        </AnchorPane>
                    </children>
                </VBox>
            </left>
            <center>
                <AnchorPane fx:id="centerPane" prefHeight="600.0" prefWidth="688.0" BorderPane.alignment="CENTER"/>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
