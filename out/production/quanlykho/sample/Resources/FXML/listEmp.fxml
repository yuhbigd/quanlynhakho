<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pane" prefHeight="600.0" prefWidth="688.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.listEmpController">
    <children>
        <BorderPane prefHeight="600.0" prefWidth="688.0">
            <center>
                <TableView fx:id="listEmp" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="570.0" prefWidth="647.0" BorderPane.alignment="CENTER">
                    <columns>
                        <TableColumn fx:id="userNameCol" prefWidth="91.0" text="Username" />
                        <TableColumn fx:id="passCol" prefWidth="91.0" text="Password" />
                        <TableColumn fx:id="posCol" prefWidth="95.0" text="Chức vụ" />
                        <TableColumn fx:id="sexCol" prefWidth="74.0" text="Giới Tính" />
                        <TableColumn fx:id="pNumCol" prefWidth="122.0" text="Số điện thoại" />
                        <TableColumn fx:id="lCol" prefWidth="86.0" text="Nơi ở" />
                  <TableColumn fx:id="nameCol" prefWidth="128.0" text="Họ Tên" />
                    </columns>
                </TableView>
            </center>
            <top>
                <AnchorPane prefHeight="30.0" prefWidth="600.0" BorderPane.alignment="CENTER">
                    <children>
                        <TextField fx:id="search_tf" layoutX="432.0" layoutY="4.0" prefHeight="25.0" prefWidth="185.0" promptText="Tìm bằng tên hoặc username" />
                        <Button fx:id="searchBtn" layoutX="624.0" layoutY="4.0" mnemonicParsing="false" onAction="#searchAction" text="search" />
                        <Button fx:id="addBtn" layoutX="22.0" layoutY="4.0" mnemonicParsing="false" onAction="#addingAction" text="Thêm" />
                        <Button fx:id="updateBtn" layoutX="122.0" layoutY="3.0" mnemonicParsing="false" onAction="#updatingAction" prefHeight="25.0" prefWidth="56.0" text="Sửa" />
                  <Button layoutX="233.0" layoutY="4.0" mnemonicParsing="false" onAction="#deleteAction" prefHeight="25.0" prefWidth="59.0" text="Xoá" />
                    </children>
                </AnchorPane>
            </top>
        </BorderPane>
    </children>
</AnchorPane>
