               <children>
                  <Button fx:id="homeButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#homeButtonOnClick" prefHeight="80.0" prefWidth="100.0" text="Thống kê chung" />
